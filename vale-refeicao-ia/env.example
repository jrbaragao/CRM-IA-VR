# Configurações do Sistema de Agente de IA

# OpenAI API
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# Modelos disponíveis:
# - gpt-4-turbo-preview (recomendado)
# - gpt-4-1106-preview
# - gpt-4o (se disponível)
# - gpt-3.5-turbo (mais barato, menos capaz)

# Banco de dados (SQLite para teste)
DATABASE_URL=sqlite:///./vale_refeicao.db

# Debug
DEBUG=False

# Upload
MAX_FILE_SIZE_MB=500

# Agentes
AGENT_TEMPERATURE=0.3
AGENT_MAX_RETRIES=3
